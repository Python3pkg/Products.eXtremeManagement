<dl class="portlet portletProjectPortlet"
    i18n:domain="eXtremeManagement">
  <dt class="portletHeader">
    <span class="portletTopLeft"></span>
    <span class="management_portletHeader" i18n:translate="head_iterations">Iterations</span>
    <span class="portletTopRight"></span>
  </dt>

  <dd class="portletItem Even">
    <a tal:attributes="href view/project_url"
       i18n:translate="label_current_iteration">Current</a>
  </dd>

  <dd class="portletItem Odd">
    <a tal:attributes="href view/project_url"
       i18n:translate="label_new_iteration">New</a>
  </dd>

  <dd class="portletItem Even">
    <a tal:attributes="href view/project_url"
       i18n:translate="label_closed_iteration">Closed</a>
  </dd>

  <dd class="portletItem Odd">
    <a tal:attributes="href view/project_url"
       i18n:translate="label_release_plan">Release Plan</a>
  </dd>

  <dd class="portletFooter">
    <span class="myProject"></span>
    <span class="portletBottomLeft"></span>
    <span class="portletBottomRight"></span>
  </dd>
</dl>
