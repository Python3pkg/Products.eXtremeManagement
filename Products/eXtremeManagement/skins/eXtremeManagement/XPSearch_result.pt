         <div tal:condition="results">
           <table class="listing" 
                  border="1"
                  cellpadding="0" cellspacing="0">
     
             <thead>
               <tr>
                 <th>test</th>
                 <th>test 2</th>
               </tr>
             </thead>

             <tbody>
               <tr>
                 <td>test</td>
                 <td>test 2</td>
