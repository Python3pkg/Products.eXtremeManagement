#schedule-wrapper {
  border-color: #d1d2cd;
  border-style: solid;
  border-width: 1px 0 1px 0;
  float: left;
  width: 690px;
  margin-top: 1ex;
}
#schedule-labels {
  width: 129px;
  background-color: #ebebe9;
  float: left;
  border-left: 1px solid #d1d2cd;
  border-bottom: 1px solid #d1d2cd;
}
#schedule-field {
  overflow: hidden;
  width: 559px; /* 5*112px */
  border-right: 1px solid #d1d2cd;
  clear: right;
  float: left;
}
#schedule-containment {
  position: relative;
/* Set in code
  left: -800px;
  width: 2400px;*/
}
#schedule-bars {
  position: relative;
/* Set in code
  left: 800px;
  width: 1600px; */
  background-color: #fff;
}

#schedule-dates {
  background: #fff;
  border-bottom: 1px solid #99bbe8;
}

  .schedule-date {
  float: left;
  width: 112px; /* 7 x 16px */
  background: url(&dtml-portal_url;/black-dot.gif) repeat-y right top;
  font-size: 90%;
  line-height: 1.5em;
  text-indent: 1em;
}

.schedule-label, .schedule-bar, #schedule-dates, .schedule-date, .draggable-iteration-bar, .undraggable-iteration-bar {
height: 32px;
}
.schedule-label.header,  #schedule-dates {
height: 18px;
background: url(&dtml-portal_url;/schedule-bumber.gif) repeat-y right top;
}
.schedule-label {
background: url(&dtml-portal_url;/schedule-label.gif) repeat-x right top;
}
.schedule-label a {
font-weight: bold;
display: table-cell;
vertical-align: middle;
height: 30px;
padding-left: 10px;
}
.schedule-label.header {
background: url(&dtml-portal_url;/schedule-label-header.gif) repeat-x right top;
}
.schedule-bar {
background: url(&dtml-portal_url;/color-grid.gif);
position: relative;
}
.draggable-iteration-bar, .undraggable-iteration-bar {
width: 80px;
position: absolute;
line-height: 2.5em;
text-indent: 1em;
}
.undraggable-iteration-bar {
background-color: #fbb;
}
.draggable-iteration-bar {
background-color: #bbf;
}
.ui-draggable-overlapped {
background-color: red;
}
.ui-draggable-overlapping {
background-color: green;
}

.ui-draggable-dragging {
background-color: orange;
border-color: #99bbe8;
border-style: solid;
border-width: 0 1px 0 1px;
}

.current-date {
    background-color: #e4f8c4;
}

#period-selection {
    margin-top: 2ex;
}

#project_list_widget {
    border: 1px solid gray;
    right: 10px;
    position: absolute;
    z-index: 100;
    background-color: white;
    padding: 5px;
}

#project_list_widget #widget_listing {
    display: none;
}

/* </dtml-with> */
